{
  "name": "nodejs-exercise-tracker",
  "version": "0.2.1",
  "description": "A full stack application that tracks exercise behavior.",
  "main": "server.ts",
  "scripts": {
    "start": "nodemon ./src/server.ts",
    "format": "prettier --write \"src/**/*.ts\"",
    "patch-version": "npm --no-git-tag-version version patch",
    "minor-version": "npm --no-git-tag-version version minor",
    "major-version": "npm --no-git-tag-version version major",
    "prepare": "husky install"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.16.4",
    "mongoose": "^5.11.15"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/node": "^20.3.1",
    "husky": "^8.0.3",
    "lint-staged": "^15.1.0",
    "nodemon": "^3.0.1",
    "prettier": "^3.0.0",
    "ts-loader": "^9.4.3",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.1.3"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/christophermilian/nodejs-exercise-tracker.git"
  },
  "keywords": [
    "node",
    "express",
    "mongodb"
  ],
  "author": "Christopher Milian",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/christophermilian/nodejs-exercise-tracker/issues"
  },
  "homepage": "https://github.com/christophermilian/nodejs-exercise-tracker#readme"
}
